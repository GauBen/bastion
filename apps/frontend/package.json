{
  "name": "frontend",
  "version": "0.0.1",
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.68",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cookie": "^0.4.2",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sanitize.css": "^13.0.0",
    "sass": "^1.46.0",
